local function callback(Text)
 if Text == "Yes" then
  loadstring(game:HttpGet("https://raw.githubusercontent.com/Yopapa699/spectrum-Hub/main/Spectrum%20Hub"))();
elseif Text == ("No") then
 print ("Closed") -- what second button do
 end
end

local NotificationBindable = Instance.new("BindableFunction")
NotificationBindable.OnInvoke = callback
--
game.StarterGui:SetCore("SendNotification",  {
 Title = "spectrum hub";
 Text = "do you want to execute spectrum hub?";
 Icon = "";
 Duration = 99;
 Button1 = "Yes";
 Button2 = "No";
 Callback = NotificationBindable;
})
